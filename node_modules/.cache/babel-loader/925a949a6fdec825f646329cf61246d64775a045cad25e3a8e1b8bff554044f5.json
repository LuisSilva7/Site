{"ast":null,"code":"import TheHeader from '../../components/frontoffice/TheHeader.vue';\nimport IniciativeForm from '../../components/frontoffice/IniciativeForm.vue';\nimport IniciativeInfo from '../../components/frontoffice/IniciativeInfo.vue';\nimport { getAuth, onAuthStateChanged } from \"firebase/auth\";\nexport default {\n  components: {\n    IniciativeForm,\n    IniciativeInfo,\n    TheHeader\n  },\n  data() {\n    return {\n      proposedIniciatives: [],\n      userEmail: ''\n    };\n  },\n  created() {\n    if (localStorage.getItem('proposedIniciatives')) {\n      const auth = getAuth();\n      onAuthStateChanged(auth, user => {\n        if (user) {\n          this.userEmail = user.email;\n          this.proposedIniciatives = JSON.parse(localStorage.getItem('proposedIniciatives')).filter(proposedIniciative => proposedIniciative.email.includes(this.userEmail));\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["TheHeader","IniciativeForm","IniciativeInfo","getAuth","onAuthStateChanged","components","data","proposedIniciatives","userEmail","created","localStorage","getItem","auth","user","email","JSON","parse","filter","proposedIniciative","includes"],"sources":["C:\\Users\\Luís\\Desktop\\Site\\src\\views\\frontoffice\\IniciativeForm.vue"],"sourcesContent":["<template>\n  <TheHeader />\n    <div class=\"page-container\">\n      <IniciativeForm />\n      <div>\n        <h3>AS MINHAS INICIATIVAS</h3>\n        <div v-for=\"proposedIniciative in proposedIniciatives\" :key=\"proposedIniciative.name\">\n          <IniciativeInfo />\n        </div>\n      </div>\n    </div>\n  </template>\n  \n  <script>\n  import TheHeader from '../../components/frontoffice/TheHeader.vue'\n  import IniciativeForm from '../../components/frontoffice/IniciativeForm.vue';\n  import IniciativeInfo from '../../components/frontoffice/IniciativeInfo.vue'\n  import { getAuth, onAuthStateChanged } from \"firebase/auth\"\n  \n  export default {\n      components: { IniciativeForm, IniciativeInfo, TheHeader },\n      data() {\n        return {\n          proposedIniciatives: [],\n          userEmail: ''\n        }\n      },\n      created() {\n        if (localStorage.getItem('proposedIniciatives')) {\n          const auth = getAuth()\n          onAuthStateChanged(auth, (user) => {\n            if (user) {\n              this.userEmail = user.email\n\n              this.proposedIniciatives = JSON.parse(localStorage.getItem('proposedIniciatives'))\n              .filter(proposedIniciative => proposedIniciative.email.includes(this.userEmail))\n            }\n          })\n        }\n      }\n  }\n  </script>\n  \n  <style scoped>\n  .page-container {\n    margin-right: 30%;\n    display: flex;\n    justify-content: flex-start; /* Alinha o conteúdo à esquerda */\n    padding-left: 20px; /* Adiciona um espaço à esquerda */\n  }\n  </style>"],"mappings":"AAcE,OAAOA,SAAQ,MAAO,4CAA2C;AACjE,OAAOC,cAAa,MAAO,iDAAiD;AAC5E,OAAOC,cAAa,MAAO,iDAAgD;AAC3E,SAASC,OAAO,EAAEC,kBAAiB,QAAS,eAAc;AAE1D,eAAe;EACXC,UAAU,EAAE;IAAEJ,cAAc;IAAEC,cAAc;IAAEF;EAAU,CAAC;EACzDM,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,mBAAmB,EAAE,EAAE;MACvBC,SAAS,EAAE;IACb;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAIC,YAAY,CAACC,OAAO,CAAC,qBAAqB,CAAC,EAAE;MAC/C,MAAMC,IAAG,GAAIT,OAAO,CAAC;MACrBC,kBAAkB,CAACQ,IAAI,EAAGC,IAAI,IAAK;QACjC,IAAIA,IAAI,EAAE;UACR,IAAI,CAACL,SAAQ,GAAIK,IAAI,CAACC,KAAI;UAE1B,IAAI,CAACP,mBAAkB,GAAIQ,IAAI,CAACC,KAAK,CAACN,YAAY,CAACC,OAAO,CAAC,qBAAqB,CAAC,EAChFM,MAAM,CAACC,kBAAiB,IAAKA,kBAAkB,CAACJ,KAAK,CAACK,QAAQ,CAAC,IAAI,CAACX,SAAS,CAAC;QACjF;MACF,CAAC;IACH;EACF;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}