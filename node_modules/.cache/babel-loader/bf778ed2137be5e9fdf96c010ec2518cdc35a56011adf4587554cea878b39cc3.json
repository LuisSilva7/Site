{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport BlueButton from './BlueButton.vue';\nimport RedButton from './RedButton.vue';\nimport CreateIniciativePlan from './CreateIniciativePlan.vue';\nexport default {\n  props: ['proposedIniciative', 'status'],\n  components: {\n    BlueButton,\n    RedButton,\n    CreateIniciativePlan\n  },\n  data() {\n    return {\n      createPlan: false\n    };\n  },\n  methods: {\n    closeHandler() {\n      this.$emit('close', '');\n    },\n    rejectHandler() {\n      if (localStorage.getItem('proposedIniciatives')) {\n        let proposedIniciatives = JSON.parse(localStorage.getItem('proposedIniciatives')).filter(proposedIniciative => proposedIniciative.theme !== this.proposedIniciative.theme);\n        this.proposedIniciative.status = 'rejeitada';\n        proposedIniciatives.push(this.proposedIniciative);\n        localStorage.setItem('proposedIniciatives', JSON.stringify(proposedIniciatives));\n        location.reload();\n      }\n    },\n    createIniciativePlan() {\n      this.createPlan = true;\n      let iniciative = this.proposedIniciative;\n      this.$emit('createPlan', iniciative);\n    }\n  }\n};","map":{"version":3,"names":["BlueButton","RedButton","CreateIniciativePlan","props","components","data","createPlan","methods","closeHandler","$emit","rejectHandler","localStorage","getItem","proposedIniciatives","JSON","parse","filter","proposedIniciative","theme","status","push","setItem","stringify","location","reload","createIniciativePlan","iniciative"],"sources":["C:\\Users\\Luís\\Desktop\\SitePW-main\\src\\components\\backoffice\\ProposedIniciativeBoxConsulted.vue"],"sourcesContent":["<template>\n  <div class=\"card mb-3\" style=\"max-width: 50rem; max-height:50rem\">\n    <div class=\"row g-0\">\n      <div class=\"col-md-4 d-flex\">\n        <img src=\"@/assets/gym-class-image.png\" class=\"img-fluid rounded-start\" alt=\"gym class\">\n      </div>\n        <div class=\"col-md-8 d-flex flex-column\">\n          <div class=\"card-body\">\n            <div class=\"button-body mt-auto\">\n            <BlueButton @click=\"closeHandler\" content=\"-\" />\n            </div>\n            <h5 class=\"card-title\">{{ proposedIniciative.theme }}</h5>\n            <p class=\"card-text\">DIA: {{ proposedIniciative.date }}</p>\n            <p class=\"card-text\">LOCAL: {{ proposedIniciative.local }}</p>\n            <p class=\"card-text\">OBJETIVO: {{ proposedIniciative.objective }}</p>\n            <p class=\"card-text\">DADOS PROPONENTE:</p>\n            <p class=\"card-text\">NOME: {{ proposedIniciative.name }}</p>\n            <p class=\"card-text\">EMAIL: {{ proposedIniciative.email }}</p>\n            <p class=\"card-text\">DATA NASCIMENTO: {{ proposedIniciative.birthDate }}</p>\n            <p class=\"card-text\">MÍNIMO PARTICIPANTES: {{ proposedIniciative.minParticipants }}</p>\n            <p class=\"card-text\">MÁXIMO PARTICIPANTES: {{ proposedIniciative.maxParticipants }}</p>\n          </div>\n          <div v-if=\"!status\">\n            <div>\n              <RedButton @click=\"rejectHandler\" content=\"RECUSAR\" />\n            </div>\n            <div class=\"button-body mt-auto\">\n              <BlueButton @click=\"createIniciativePlan\" content=\"CRIAR PLANO\" />\n            </div>\n          </div>\n        </div>\n    </div>\n  </div>\n</template>\n\n <script>\nimport BlueButton from './BlueButton.vue'\nimport RedButton from './RedButton.vue'\nimport CreateIniciativePlan from './CreateIniciativePlan.vue'\n\n  export default {\n    props: ['proposedIniciative', 'status'],\n    components: { BlueButton, RedButton, CreateIniciativePlan },\n    data() {\n        return {\n            createPlan: false\n        }\n    },\n    methods: {\n        closeHandler() {\n            this.$emit('close', '')\n        },\n        rejectHandler() {\n            if (localStorage.getItem('proposedIniciatives')) {\n                let proposedIniciatives = JSON.parse(localStorage.getItem('proposedIniciatives'))\n                                            .filter(proposedIniciative => proposedIniciative.theme !== this.proposedIniciative.theme)\n                this.proposedIniciative.status = 'rejeitada'\n                proposedIniciatives.push(this.proposedIniciative)\n                localStorage.setItem('proposedIniciatives', JSON.stringify(proposedIniciatives))\n\n                location.reload()\n            }\n        },\n        createIniciativePlan() {\n            this.createPlan = true\n            let iniciative = this.proposedIniciative\n            this.$emit('createPlan', iniciative)\n        }\n    }\n  }\n  </script>\n \n <style scoped>\n\n .card.mb-3 { \n height: 10.2rem;\n display: flex;\n flex-direction: row;\n margin-left: 2.5rem;\n margin-top:10rem; \n border-radius: 10px;\n overflow:visible;\n background-color: #1B2631;\n }\n \n .col-mg-4 {\n height:100%;\n margin-right: 0%;\n width:45%;\n }\n \n .col-mg-4 img{\n object-fit: cover;\n }\n \n .col-md-8 {\n width: 55%;\n color: white;\n font-family: nunito;\n font-size:18px;\n height:fit-content;\n }\n \n .card-body {\n padding: 10px;\n }\n \n .card-title,\n .card-text {\n margin-bottom: 10px; /* Adjust the margin bottom for each element */\n }\n \n .btn {\n color: white;\n font-size:13px;\n font-style: bold;\n border:#161934;\n border-radius: 50px;\n background-image: linear-gradient(to bottom, #414A9A, #161934);\n margin-left: 96%;\n margin-bottom: 2%;\n }\n \n </style>"],"mappings":";AAoCA,OAAOA,UAAS,MAAO,kBAAiB;AACxC,OAAOC,SAAQ,MAAO,iBAAgB;AACtC,OAAOC,oBAAmB,MAAO,4BAA2B;AAE1D,eAAe;EACbC,KAAK,EAAE,CAAC,oBAAoB,EAAE,QAAQ,CAAC;EACvCC,UAAU,EAAE;IAAEJ,UAAU;IAAEC,SAAS;IAAEC;EAAqB,CAAC;EAC3DG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,UAAU,EAAE;IAChB;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,YAAYA,CAAA,EAAG;MACX,IAAI,CAACC,KAAK,CAAC,OAAO,EAAE,EAAE;IAC1B,CAAC;IACDC,aAAaA,CAAA,EAAG;MACZ,IAAIC,YAAY,CAACC,OAAO,CAAC,qBAAqB,CAAC,EAAE;QAC7C,IAAIC,mBAAkB,GAAIC,IAAI,CAACC,KAAK,CAACJ,YAAY,CA