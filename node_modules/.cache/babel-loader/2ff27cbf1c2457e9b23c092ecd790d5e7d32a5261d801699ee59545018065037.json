{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Button from '../../components/frontoffice/Button.vue';\nimport TheHeader from '../../components/frontoffice/TheHeader.vue';\nimport { getAuth, onAuthStateChanged } from \"firebase/auth\";\nexport default {\n  components: {\n    TheHeader,\n    Button\n  },\n  data() {\n    return {\n      //fazer login e registar email e ver foto\n      proposedCollaborator: {\n        photo: '',\n        name: '',\n        birthDate: '',\n        telNumber: '',\n        address: '',\n        email: ''\n      },\n      file: null,\n      ImageData: ''\n    };\n  },\n  methods: {\n    proposedCollaboratorHandler() {\n      if (localStorage.getItem('proposedCollaborators')) {\n        const auth = getAuth();\n        onAuthStateChanged(auth, user => {\n          if (user) {\n            const reader = new FileReader();\n            reader.onload = event => {\n              const base64Image = event.target.result;\n              this.proposedCollaborator.email = user.email;\n              let proposedCollaborators = JSON.parse(localStorage.getItem('proposedCollaborators'));\n              this.proposedCollaborator.photo = base64Image;\n              proposedCollaborators.push(this.proposedCollaborator);\n              localStorage.setItem('proposedCollaborators', JSON.stringify(proposedCollaborators));\n            };\n            if (this.file) {\n              reader.readAsDataURL(this.file);\n              this.$router.push('/');\n              alert(\"Candidatura a colaborador enviada com sucesso!\");\n            } else {\n              this.proposedCollaborator.email = user.email;\n              let proposedCollaborators = JSON.parse(localStorage.getItem('proposedCollaborators'));\n              proposedCollaborators.push(this.proposedCollaborator);\n              localStorage.setItem('proposedCollaborators', JSON.stringify(proposedCollaborators));\n              this.$router.push('/');\n              alert(\"Candidatura a colaborador enviada com sucesso!\");\n            }\n          }\n        });\n      }\n    },\n    handleFileChange(event) {\n      this.file = event.target.files[0]; // Obtém o arquivo de imagem selecionado pelo usuário\n    }\n  }\n};","map":{"version":3,"names":["Button","TheHeader","getAuth","onAuthStateChanged","components","data","proposedCollaborator","photo","name","birthDate","telNumber","address","email","file","ImageData","methods","proposedCollaboratorHandler","localStorage","getItem","auth","user","reader","FileReader","onload","event","base64Image","target","result","proposedCollaborators","JSON","parse","push","setItem","stringify","readAsDataURL","$router","alert","handleFileChange","files"],"sources":["C:\\Users\\Luís\\Desktop\\Site\\src\\views\\frontoffice\\CollaboratorForm.vue"],"sourcesContent":["<template>\n  <TheHeader />\n  <div id=\"collaborator\" class=\"container\">\n    <div class=\"rectangle\">\n      <h6>CANDIDATE-SE A COLABORADOR</h6>\n      <p>Vais conseguir ajudar pessoas por todo o mundo. Faz parte desta iniciativa!</p>\n      <div class=\"form-container\">\n        <form>\n          <input v-model=\"proposedCollaborator.name\" type=\"text\" placeholder=\"Nome\">\n          <input v-model=\"proposedCollaborator.birthDate\" type=\"date\" placeholder=\"Data de Nascimento\">\n          <input v-model=\"proposedCollaborator.telNumber\" type=\"tel\" placeholder=\"Telefone\">\n          <input v-model=\"proposedCollaborator.address\" type=\"text\" placeholder=\"Morada\">\n          <input type=\"file\" @change=\"handleFileChange\" id=\"foto\" name=\"foto\" accept=\"image/*\" required>\n        </form>\n      </div>\n      <Button @click=\"proposedCollaboratorHandler\" content=\"JUNTA-TE A NÓS\"/>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Button from '../../components/frontoffice/Button.vue'\nimport TheHeader from '../../components/frontoffice/TheHeader.vue'\nimport { getAuth, onAuthStateChanged } from \"firebase/auth\";\n\nexport default {\n  components: { TheHeader, Button },\n  data() {\n    return {\n      //fazer login e registar email e ver foto\n      proposedCollaborator: {photo: '', name: '', birthDate: '', telNumber: '', address: '', email: ''},\n      file: null,\n      ImageData: ''\n    }\n  },\n  methods: {\n    proposedCollaboratorHandler() {\n        if (localStorage.getItem('proposedCollaborators')) {\n          const auth = getAuth()\n          onAuthStateChanged(auth, (user) => {\n            if (user) {\n\n              const reader = new FileReader()\n\n              reader.onload = (event) => {\n\n                const base64Image = event.target.result;\n\n                this.proposedCollaborator.email = user.email\n\n                let proposedCollaborators = JSON.parse(localStorage.getItem('proposedCollaborators'))\n                this.proposedCollaborator.photo = base64Image\n                proposedCollaborators.push(this.proposedCollaborator)\n                localStorage.setItem('proposedCollaborators', JSON.stringify(proposedCollaborators))\n              }\n              if(this.file) {\n                reader.readAsDataURL(this.file)\n                this.$router.push('/')\n                alert(\"Candidatura a colaborador enviada com sucesso!\")\n              }\n              else {\n                this.proposedCollaborator.email = user.email\n                let proposedCollaborators = JSON.parse(localStorage.getItem('proposedCollaborators'))\n                proposedCollaborators.push(this.proposedCollaborator)\n                localStorage.setItem('proposedCollaborators', JSON.stringify(proposedCollaborators))\n                this.$router.push('/')\n                alert(\"Candidatura a colaborador enviada com sucesso!\")\n              }\n            }\n          })\n        }\n      },\n      handleFileChange(event) {\n        this.file = event.target.files[0]; // Obtém o arquivo de imagem selecionado pelo usuário\n      }\n  }\n}\n</script>\n\n<style scoped>\n.container {\n  display: flex;\n  justify-content: center;\n}\n\n.rectangle {\n  width: 400px; /* Largura do retângulo */\n  padding: 20px;\n  background-color: rgba(201, 164, 164, 0.1); /* Cor de fundo com transparência */\n  border: 2px solid #333; /* Borda sólida de 2px */\n  border-radius: 10px; /* Borda arredondada */\n  text-align: center; /* Alinhamento do texto ao centro */\n}\n\nh6 {\n  margin-bottom: 10px; /* Espaçamento inferior entre o título e o texto */\n}\n\np {\n  font-style: italic; /* Texto em itálico */\n}\n\n.form-container {\n  margin-bottom: 20px; /* Espaçamento entre o formulário e o botão */\n}\n</style>"],"mappings":";AAqBA,OAAOA,MAAK,MAAO,yCAAwC;AAC3D,OAAOC,SAAQ,MAAO,4CAA2C;AACjE,SAASC,OAAO,EAAEC,kBAAiB,QAAS,eAAe;AAE3D,eAAe;EACbC,UAAU,EAAE;IAAEH,SAAS;IAAED;EAAO,CAAC;EACjCK,IAAIA,CAAA,EAAG;IACL,OAAO;MACL;MACAC,oBAAoB,EAAE;QAACC,KAAK,EAAE,EAAE;QAAEC,IAAI,EAAE,EAAE;QAAEC,SAAS,EAAE,EAAE;QAAEC,SAAS,EAAE,EAAE;QAAEC,OAAO,EAAE,EAAE;QAAEC,KAAK,EAAE;MAAE,CAAC;MACjGC,IAAI,EAAE,IAAI;MACVC,SAAS,EAAE;IACb;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,2BAA2BA,CAAA,EAAG;MAC1B,IAAIC,YAAY,CAACC,OAAO,CAAC,uBAAuB,CAAC,EAAE;QACjD,MAAMC,IAAG,GAAIjB,OAAO,CAAC;QACrBC,kBAAkB,CAACgB,IAAI,EAAGC,IAAI,IAAK;UACjC,IAAIA,IAAI,EAAE;YAER,MAAMC,MAAK,GAAI,IAAIC,UAAU,CAAC;YAE9BD,MAAM,CAACE,MAAK,GAAKC,KAAK,IAAK;cAEzB,MAAMC,WAAU,GAAID,KAAK,CAACE,MAAM,CAACC,MAAM;cAEvC,IAAI,CAACrB,oBAAoB,CAACM,KAAI,GAAIQ,IAAI,CAACR,KAAI;cAE3C,IAAIgB,qBAAoB,GAAIC,IAAI,CAACC,KAAK,CAACb,YAAY,CAACC,OAAO,CAAC,uBAAuB,CAAC;cACpF,IAAI,CAACZ,oBAAoB,CAACC,KAAI,GAAIkB,WAAU;cAC5CG,qBAAqB,CAACG,IAAI,CAAC,IAAI,CAACzB,oBAAoB;cACpDW,YAAY,CAACe,OAAO,CAAC,uBAAuB,EAAEH,IAAI,CAACI,SAAS,CAACL,qBAAqB,CAAC;YACrF;YACA,IAAG,IAAI,CAACf,IAAI,EAAE;cACZQ,MAAM,CAACa,aAAa,CAAC,IAAI,CAACrB,IAAI;cAC9B,IAAI,CAACsB,OAAO,CAACJ,IAAI,CAAC,GAAG;cACrBK,KAAK,CAAC,gDAAgD;YACxD,OACK;cACH,IAAI,CAAC9B,oBAAoB,CAACM,KAAI,GAAIQ,IAAI,CAACR,KAAI;cAC3C,IAAIgB,qBAAoB,GAAIC,IAAI,CAACC,KAAK,CAACb,YAAY,CAACC,OAAO,CAAC,uBAAuB,CAAC;cAC